{
  "name": "blr-toll",
  "version": "1.0.0",
  "description": "BLR TOLL prosjekt med Excel import og database matching",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev-full": "concurrently \"nodemon server.js\" \"cd client && npm run serve\"",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "setup": "npm install && npm run install-client",
    "db-setup": "node database/setup-db.js"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "vuetify": "^3.9.0-beta.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "toll",
    "excel",
    "database",
    "vue",
    "nodejs"
  ],
  "author": "BLR",
  "license": "MIT"
}
